GOutputStream"},{"type":"text","text":")."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/File","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"File"}],"url":"\/documentation\/gio\/file"},"doc://GIO/documentation/GIO":{"role":"collection","title":"GIO","abstract":[],"identifier":"doc:\/\/GIO\/documentation\/GIO","kind":"symbol","type":"topic","url":"\/documentation\/gio"},"doc://GIO/documentation/GIO/File/FileProtocol-Implementations":{"role":"collectionGroup","title":"FileProtocol Implementations","abstract":[],"identifier":"doc:\/\/GIO\/documentation\/GIO\/File\/FileProtocol-Implementations","kind":"article","type":"topic","url":"\/documentation\/gio\/file\/fileprotocol-implementations"},"doc://GIO/documentation/GIO/File/enumerateChildren(attributes:flags:cancellable:)":{"role":"symbol","title":"enumerateChildren(attributes:flags:cancellable:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"enumerateChildren"},{"kind":"text","text":"("},{"kind":"externalParam","text":"attributes"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UnsafePointer","preciseIdentifier":"s:SP"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"CChar","preciseIdentifier":"s:s5CChara"},{"kind":"text","text":">!, "},{"kind":"externalParam","text":"flags"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"FileQueryInfoFlags","preciseIdentifier":"s:3GIO18FileQueryInfoFlagsV"},{"kind":"text","text":", "},{"kind":"externalParam","text":"cancellable"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"CancellableRef","preciseIdentifier":"s:3GIO14CancellableRefV"},{"kind":"text","text":"?) "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"kind":"typeIdentifier","text":"FileEnumeratorRef","preciseIdentifier":"s:3GIO17FileEnumeratorRefV"},{"kind":"text","text":"!"}],"abstract":[{"type":"text","text":"Gets the requested information about the files in a directory."},{"type":"text","text":" "},{"type":"text","text":"The result is a "},{"type":"codeVoice","code":"GFileEnumerator"},{"type":"text","text":" object that will give out"},{"type":"text","text":" "},{"type":"codeVoice","code":"GFileInfo"},{"type":"text","text":" objects for all the files in the directory."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/File\/enumerateChildren(attributes:flags:cancellable:)","kind":"symbol","type":"topic","url":"\/documentation\/gio\/file\/enumeratechildren(attributes:flags:cancellable:)"}}}