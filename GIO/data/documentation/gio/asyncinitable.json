; l = l->next)","    {","      GTask *task = l->data;","","      if (self->priv->success)","        g_task_return_boolean (task, TRUE);","      else","        g_task_return_new_error (task, ...);","      g_object_unref (task);","    }","","  g_list_free (self->priv->init_results);","  self->priv->init_results = NULL;","}","","static void","foo_init_async (GAsyncInitable       *initable,","                int                   io_priority,","                GCancellable         *cancellable,","                GAsyncReadyCallback   callback,","                gpointer              user_data)","{","  Foo *self = FOO (initable);","  GTask *task;","","  task = g_task_new (initable, cancellable, callback, user_data);","  g_task_set_name (task, G_STRFUNC);","","  switch (self->priv->state)","    {","      case NOT_INITIALIZED:","        _foo_get_ready (self);","        self->priv->init_results = g_list_append (self->priv->init_results,","                                                  task);","        self->priv->state = INITIALIZING;","        break;","      case INITIALIZING:","        self->priv->init_results = g_list_append (self->priv->init_results,","                                                  task);","        break;","      case INITIALIZED:","        if (!self->priv->success)","          g_task_return_new_error (task, ...);","        else","          g_task_return_boolean (task, TRUE);","        g_object_unref (task);","        break;","    }","}","","static gboolean","foo_init_finish (GAsyncInitable       *initable,","                 GAsyncResult         *result,","                 GError              **error)","{","  g_return_val_if_fail (g_task_is_valid (result, initable), FALSE);","","  return g_task_propagate_boolean (G_TASK (result), error);","}","","static void","foo_async_initable_iface_init (gpointer g_iface,","                               gpointer data)","{","  GAsyncInitableIface *iface = g_iface;","","  iface->init_async = foo_init_async;","  iface->init_finish = foo_init_finish;","}"]},{"type":"paragraph","inlineContent":[{"type":"text","text":"The "},{"type":"codeVoice","code":"AsyncInitable"},{"type":"text","text":" type acts as an owner of an underlying "},{"type":"codeVoice","code":"GAsyncInitable"},{"type":"text","text":" instance."},{"type":"text","text":" "},{"type":"text","text":"It provides the methods that can operate on this data type through "},{"type":"codeVoice","code":"AsyncInitableProtocol"},{"type":"text","text":" conformance."},{"type":"text","text":" "},{"type":"text","text":"Use "},{"type":"codeVoice","code":"AsyncInitable"},{"type":"text","text":" as a strong reference or owner of a "},{"type":"codeVoice","code":"GAsyncInitable"},{"type":"text","text":" instance."}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/gio\/asyncinitable"],"traits":[{"interfaceLanguage":"swift"}]}],"relationshipsSections":[{"identifiers":["doc:\/\/GIO\/documentation\/GIO\/AsyncInitableProtocol","doc:\/\/GIO\/4GLib14PointerWrapperP"],"kind":"relationships","title":"Conforms To","type":"conformsTo"}],"identifier":{"url":"doc:\/\/GIO\/documentation\/GIO\/AsyncInitable","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"This is the asynchronous version of "},{"type":"codeVoice","code":"GInitable"},{"type":"text","text":"; it behaves the same"},{"type":"text","text":" "},{"type":"text","text":"in all ways except that initialization is asynchronous. For more details"},{"type":"text","text":" "},{"type":"text","text":"see the descriptions on "},{"type":"codeVoice","code":"GInitable"},{"type":"text","text":"."}],"kind":"symbol","metadata":{"fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"AsyncInitable"}],"title":"AsyncInitable","roleHeading":"Class","role":"symbol","symbolKind":"class","externalID":"s:3GIO13AsyncInitableC","modules":[{"name":"GIO"}],"navigatorTitle":[{"kind":"identifier","