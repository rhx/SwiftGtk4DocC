entifier":"doc:\/\/GIO\/documentation\/GIO\/TLSCertificateFlags","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"TLSCertificateFlags"}],"url":"\/documentation\/gio\/tlscertificateflags"},"doc://GIO/documentation/GIO/TLSFileDatabaseRef":{"role":"symbol","title":"TLSFileDatabaseRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"TLSFileDatabaseRef"}],"abstract":[{"type":"codeVoice","code":"GTlsFileDatabase"},{"type":"text","text":" is implemented by "},{"type":"codeVoice","code":"GTlsDatabase"},{"type":"text","text":" objects which load"},{"type":"text","text":" "},{"type":"text","text":"their certificate information from a file. It is an interface which"},{"type":"text","text":" "},{"type":"text","text":"TLS library specific subtypes implement."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/TLSFileDatabaseRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"TLSFileDatabaseRef"}],"url":"\/documentation\/gio\/tlsfiledatabaseref"},"doc://GIO/documentation/GIO/CancellableProtocol":{"role":"symbol","title":"CancellableProtocol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"CancellableProtocol"}],"abstract":[{"type":"text","text":"GCancellable is a thread-safe operation cancellation stack used"},{"type":"text","text":" "},{"type":"text","text":"throughout GIO to allow for cancellation of synchronous and"},{"type":"text","text":" "},{"type":"text","text":"asynchronous operations."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/CancellableProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"CancellableProtocol"}],"url":"\/documentation\/gio\/cancellableprotocol"},"doc://GIO/documentation/GIO/TLSFileDatabaseRef/verify(chain:purpose:identity:interaction:flags:cancellable:)-98nbf":{"role":"symbol","title":"verify(chain:purpose:identity:interaction:flags:cancellable