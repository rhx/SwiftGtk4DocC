":"text","text":" "},{"type":"text","text":"in the sample code below."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/SocketConnectableProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"SocketConnectableProtocol"}],"url":"\/documentation\/gio\/socketconnectableprotocol"},"doc://GIO/documentation/GIO":{"role":"collection","title":"GIO","abstract":[],"identifier":"doc:\/\/GIO\/documentation\/GIO","kind":"symbol","type":"topic","url":"\/documentation\/gio"},"doc://GIO/documentation/GIO/TLSCertificateProtocol":{"role":"symbol","title":"TLSCertificateProtocol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"TLSCertificateProtocol"}],"abstract":[{"type":"text","text":"A certificate used for TLS authentication and encryption."},{"type":"text","text":" "},{"type":"text","text":"This can represent either a certificate only (eg, the certificate"},{"type":"text","text":" "},{"type":"text","text":"received by a client from a server), or the combination of"},{"type":"text","text":" "},{"type":"text","text":"a certificate and a private key (which is needed when acting as a"},{"type":"text","text":" "},{"type":"codeVoice","code":"GTlsServerConnection"},{"type":"text","text":")."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/TLSCertificateProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"TLSCertificateProtocol"}],"url":"\/documentation\/gio\/tlscertificateprotocol"},"doc://GIO/documentation/GIO/TLSInteractionProtocol":{"role":"symbol","title":"TLSInteractionProtocol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"TLSInteractionProtocol"}],"abstract":[{"type":"codeVoice","code":"GTlsInteraction"},{"type":"text","text":" provides a mechanism for the TLS connection and database"},{"type":"text","text":" "},{"type":"text","text":"code to interact with the user. It can be used to ask the user 