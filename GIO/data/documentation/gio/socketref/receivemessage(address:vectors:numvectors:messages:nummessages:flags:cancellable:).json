tifier","text":"gint","preciseIdentifier":"c:gtypes.h@T@gint"},{"kind":"text","text":">!, "},{"kind":"externalParam","text":"cancellable"},{"kind":"text","text":": "},{"kind":"typeIdentifier","identifier":"doc:\/\/GIO\/documentation\/GIO\/CancellableRef","preciseIdentifier":"s:3GIO14CancellableRefV","text":"CancellableRef"},{"kind":"text","text":"? = nil) "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"kind":"typeIdentifier","text":"gssize","preciseIdentifier":"c:glibconfig.h@T@gssize"}],"languages":["swift"],"platforms":["macOS"]}]},{"kind":"content","content":[{"anchor":"discussion","level":2,"type":"heading","text":"Discussion"},{"type":"paragraph","inlineContent":[{"type":"text","text":"If "},{"type":"codeVoice","code":"address"},{"type":"text","text":" is non-"},{"type":"codeVoice","code":"nil"},{"type":"text","text":" then "},{"type":"codeVoice","code":"address"},{"type":"text","text":" will be set equal to the"},{"type":"text","text":" "},{"type":"text","text":"source address of the received packet."},{"type":"text","text":" "},{"type":"codeVoice","code":"address"},{"type":"text","text":" is owned by the caller."}]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"vector"},{"type":"text","text":" must point to an array of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputVector"},{"type":"text","text":" structs and"},{"type":"text","text":" "},{"type":"codeVoice","code":"numVectors"},{"type":"text","text":" must be the length of this array.  These structs"},{"type":"text","text":" "},{"type":"text","text":"describe the buffers that received data will be scattered into."},{"type":"text","text":" "},{"type":"text","text":"If "},{"type":"codeVoice","code":"numVectors"},{"type":"text","text":" is -1, then "},{"type":"codeVoice","code":"vectors"},{"type":"text","text":" is assumed to be terminated"},{"type":"text","text":" "},{"type":"text","text":"by a "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputVector"},{"type":"text","text":" with a "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" buffer pointer."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"As a special case, if "},{"type":"codeVoice","code":"numVectors"},{"type":"text","text":" is 0 (in which case, "},{"type":"codeVoice","code":"vectors"},{"type":"text","text":" "},{"type":"text","text":"may of course be "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"), then a single byte is received and"},{"type":"text","text":" "},{"type":"text","text":"discarded. This is to facilitate the common practice of sending a"},{"type":"text","text":" "},{"type":"text","text":"single ‘\\0’ byte for the purposes of transferring ancillary data."}]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"messages"},{"type":"text","text":", if non-"},{"type":"codeVoice","code":"nil"},{"type":"text","text":", will be set to point to a newly-allocated"},{"type":"text","text":" "},{"type":"text","text":"array of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/SocketControlMessage"},{"type":"text","text":" instances or "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" if no such"},{"type":"text","text":" "},{"type":"text","text":"messages was received. These correspond to the control messages"},{"type":"text","text":" "},{"type":"text","text":"received from the kernel, one "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/SocketControlMessage"},{"type":"text","text":" per message"},{"type":"text","text":" "},{"type":"text","text":"from the kernel. This array is "},{"type":"codeVoice","code":"nil"},{"type":"text","text":"-terminated and must be freed"},{"type":"text","text":" "},{"type":"text","text":"by the caller using "},{"type":"codeVoice","code":"g_free()"},{"type":"text","text":" after calling "},{"type":"codeVoice","code":"g_object_unref()"},{"type":"text","text":" on each"},{"type":"text","text":" "},{"type":"text","text":"element. If "},{"type":"codeVoice","code":"messages"},{"type":"text","text":" is "},{"type":"codeVoice","code":"nil"},{"type":"text","text":", any control messages received will"},{"type":"text","text":" "},{"type":"text","text":"be discarded."}]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"numMessages"},{"type":"text","text":", if non-"},{"type":"codeVoice","code":"nil"},{"type":"text","text":", will be set to the number of control"},{"type":"text","text":" "},{"type":"text","text":"messages received."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If both "},{"type":"codeVoice","code":"messages"},{"type":"text","text":" and "},{"type":"codeVoice","code":"numMessages"},{"type":"text","text":" are non-"},{"type":"codeVoice","code":"nil"},{"type":"text","text":", then"},{"type":"text","text":" "},{"type":"codeVoice","code":"numMessages"},{"type":"text","text":" gives the number of "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/SocketControlMessage"},{"type":"text","text":" instances"},{"type":"text","text":" "},{"type":"text","text":"in "},{"type":"codeVoice","code":"messages"},{"type":"text","text":" (ie: not including the "},{"type":"codeVoice","code":"nil"},{"type":"text","text":" terminator)."}]},{"type":"paragraph","inlineContent":[{"type":"codeVoice","code":"flags"},{"type":"text","text":" is an in\/out parameter. The commonly available arguments"},{"type":"text","text":" "},{"type":"text","text":"for this are available in the "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/SocketMsgFlags"},{"type":"text","text":" enum, but the"},{"type":"text","text":" "},{"type":"text","text":"values there are the same as the system values, and the flags"},{"type":"text","text":" "},{"type":"text","text":"are passed in as-is, so you can pass in system-specific flags too"},{"type":"text","text":" "},{"type":"text","text":"(and "},{"type":"codeVoice","code":"g_socket_receive_message()"},{"type":"text","text":" may pass system-specific flags out)."},{"type":"text","text":" "},{"type":"text","text":"Flags passed in to the parameter affect the receive operation; flags returned"},{"type":"text","text":" "},{"type":"text","text":"out of it are relevant to the specific returned message."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"As with "},{"type":"codeVoice","code":"g_socket_receive()"},{"type":"text","text":", data may be discarded if "},{"type":"codeVoice","code":"socket"},{"type":"text","text":" is"},{"type":"text","text":" "},{"type":"codeVoice","code":"datagram"},{"type":"text","text":" or "},{"type":"codeVoice","code":"seqpacket"},{"type":"text","text":" and you do not"},{"type":"text","text":" "},{"type":"text","text":"provide enough buffer space to read a complete message. You can pass"},{"type":"text","text":" "},{"type":"codeVoice","code":"peek"},{"type":"text","text":" in "},{"type":"codeVoice","code":"flags"},{"type":"text","text":" to peek at the current message without"},{"type":"text","text":" "},{"type":"text","text":"removing it from the receive queue, but there is no portable way to find"},{"type":"text","text":" "},{"type":"text","text":"out the length of the message other than by reading it into a"},{"type":"text","text":" "},{"type":"text","text":"sufficiently-large buffer."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"If the socket is in blocking mode the call will block until there"},{"type":"text","text":" "},{"type":"text","text":"is some data to receive, the connection is closed, or there is an"},{"type":"text","text":" "},{"type":"text","text":"error. If there is no data available and the socket is in"},{"type":"text","text":" "},{"type":"text","text":"non-blocking mode, a "},{"type":"codeVoice","code":"wouldBlock"},{"type":"text","text":" error will be"},{"type":"text","text":" "},{"type":"text","text":"returned. To be notified when data is available, wait for the"},{"type":"text","text":" "},{"type":"text","text":"[in_](\/\/documentation\/glib\/in_ condition."}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"On error -1 is returned and "},{"type":"codeVoice","code":"error"},{"type":"text","text":" is set accordingly."}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/gio\/socketref\/receivemessage(address:vectors:numvectors:messages:nummessages:flags:cancellable:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/GIO\/documentation\/GIO\/SocketRef\/receiveMessage(address:vectors:numVectors:messages:numMessages:flags:cancellable:)","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Receive data from a socket.  For receiving multiple messages, see"},{"type":"text","text":" "},{"type":"codeVoice","code":"g_socket_receive_messages()"},{"type":"text","text":"; for easier use, see"},{"type":"text","text":" "},{"type":"codeVoice","code":"g_socket_receive()"},{"type":"text","text":" and "},{"type":"codeVoice","code":"g_socket_receive_from()"},{"type":"text","text":"."}],"kind":"symbol","metadata":{"role":"symbol","title":"receiveMessage(address:vectors:numVectors:messages:numMessages:flags:cancellable:)","roleHeading":"Instance Method","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"receiveMessage"},{"kind":"text","text":"("},{"kind":"externalParam","text":"address"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UnsafeMutablePointer","preciseIdentifier":"s:Sp"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"UnsafeMutablePointer","preciseIdentifier":"s:Sp"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"GSocketAddress"},{"kind":"text","text":">?>!, "},{"kind":"externalParam","text":"vectors"},{"kind":"text","text":": "},{"kind":"typeIdentifier","