onnectionRef\/DatagramBasedProtocol-Implementations","kind":"article","type":"topic","url":"\/documentation\/gio\/dtlsserverconnectionref\/datagrambasedprotocol-implementations"},"doc://GIO/documentation/GIO/OutputVector":{"role":"symbol","title":"OutputVector","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OutputVector"}],"abstract":[{"type":"text","text":"Structure used for scatter\/gather data output."},{"type":"text","text":" "},{"type":"text","text":"You generally pass in an array of "},{"type":"codeVoice","code":"GOutputVectors"},{"type":"text","text":" "},{"type":"text","text":"and the operation will use all the buffers as if they were"},{"type":"text","text":" "},{"type":"text","text":"one buffer."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/OutputVector","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OutputVector"}],"url":"\/documentation\/gio\/outputvector"},"doc://GIO/documentation/GIO/OutputMessage":{"role":"symbol","title":"OutputMessage","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"OutputMessage"}],"abstract":[{"type":"text","text":"Structure used for scatter\/gather data output when sending multiple"},{"type":"text","text":" "},{"type":"text","text":"messages or packets in one go. You generally pass in an array of"},{"type":"text","text":" "},{"type":"codeVoice","code":"GOutputVectors"},{"type":"text","text":" and the operation will use all the buffers as if they"},{"type":"text","text":" "},{"type":"text","text":"were one buffer."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/OutputMessage","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"OutputMessage"}],"url":"\/documentation\/gio\/outputmessage"},"doc://GIO/documentation/GIO/DtlsConnection":{"role":"symbol","title":"DtlsConnection","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"DtlsConnection"}],"abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/DtlsConnection"},{"type":"text","text":" is the base DTLS connection class type, which wraps"},{"type":"text","text":" "},{"type":"text","text":"a "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/DatagramBased"},{"type":"text","text":" and provides DTLS encryption on top of it. Its"},{"type":"text","text":" "},{"type":"text","text":"subclasses, "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/DtlsClientConnection"},{"type":"text","text":" and "},{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/DtlsServerConnection"},{"type":"text","text":","},{"type":"text","text":" "},{"type":"text","text":"implement client-side and server-side DTLS, respectively."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/DtlsConnection","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"DtlsConnection"}],"url":"\/documentation\/gio\/dtlsconnection"}}}