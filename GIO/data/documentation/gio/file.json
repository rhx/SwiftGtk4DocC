f the uri type is"},{"type":"text","text":" "},{"type":"text","text":"not supported."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/File\/newFor(uri:)","kind":"symbol","type":"topic","url":"\/documentation\/gio\/file\/newfor(uri:)"},"doc://GIO/documentation/GIO/File/ptr":{"role":"symbol","title":"ptr","fragments":[{"kind":"keyword","text":"let"},{"kind":"text","text":" "},{"kind":"identifier","text":"ptr"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UnsafeMutableRawPointer","preciseIdentifier":"s:Sv"},{"kind":"text","text":"!"}],"abstract":[{"type":"text","text":"Untyped pointer to the underlying "},{"type":"codeVoice","code":"GFile"},{"type":"text","text":" instance."},{"type":"text","text":" "},{"type":"text","text":"For type-safe access, use the generated, typed pointer "},{"type":"codeVoice","code":"file_ptr"},{"type":"text","text":" property instead."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/File\/ptr","kind":"symbol","type":"topic","url":"\/documentation\/gio\/file\/ptr"},"doc://GIO/documentation/GIO/File/metatypeReferenceForFile":{"role":"symbol","title":"metatypeReferenceForFile","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"metatypeReferenceForFile"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GType","preciseIdentifier":"c:@T@GType"}],"abstract":[{"type":"text","text":"This getter returns the GLib type identifier registered for "},{"type":"codeVoice","code":"File"}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/File\/metatypeReferenceForFile","kind":"symbol","type":"topic","url":"\/documentation\/gio\/file\/metatypereferenceforfile"},"http://www.w3.org/Protocols/rfc2616/rfc2616-sec14.html":{"title":"specification","titleInlineContent":[{"type":"text","text":"specification"}],"type":"link","identifier":"http:\/\/www.w3.org\/Protocols\/rfc2616\/rfc2616-sec14.html","url":"http:\/\/www.w3.org\/Protocols\/rfc2616\/rfc2616-sec14.html"},"doc://GIO/documentation/GIO