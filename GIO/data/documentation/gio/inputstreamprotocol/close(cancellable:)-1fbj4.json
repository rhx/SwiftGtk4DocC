text":") and to skip some content"},{"type":"text","text":" "},{"type":"text","text":"("},{"type":"codeVoice","code":"g_input_stream_skip()"},{"type":"text","text":")."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputStream","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"InputStream"}],"url":"\/documentation\/gio\/inputstream"},"doc://GIO/documentation/GIO/InputStreamProtocol":{"role":"symbol","title":"InputStreamProtocol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"InputStreamProtocol"}],"abstract":[{"type":"reference","isActive":true,"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputStream"},{"type":"text","text":" has functions to read from a stream ("},{"type":"codeVoice","code":"g_input_stream_read()"},{"type":"text","text":"),"},{"type":"text","text":" "},{"type":"text","text":"to close a stream ("},{"type":"codeVoice","code":"g_input_stream_close()"},{"type":"text","text":") and to skip some content"},{"type":"text","text":" "},{"type":"text","text":"("},{"type":"codeVoice","code":"g_input_stream_skip()"},{"type":"text","text":")."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/InputStreamProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"InputStreamProtocol"}],"url":"\/documentation\/gio\/inputstreamprotocol"},"doc://GIO/documentation/GIO/CancellableRef":{"role":"symbol","title":"CancellableRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"CancellableRef"}],"abstract":[{"type":"text","text":"GCancellable is a thread-safe operation cancellation stack used"},{"type":"text","text":" "},{"type":"text","text":"throughout GIO to allow for cancellation of synchronous and"},{"type":"text","text":" "},{"type":"text","text":"asynchronous operations."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/CancellableRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"i