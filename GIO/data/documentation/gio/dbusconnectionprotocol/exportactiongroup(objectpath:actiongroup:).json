"text":": "},{"kind":"typeIdentifier","text":"ActionGroupT"},{"kind":"text","text":") "},{"kind":"keyword","text":"throws"},{"kind":"text","text":" -> "},{"kind":"typeIdentifier","text":"Int","preciseIdentifier":"s:Si"}],"abstract":[{"type":"text","text":"Exports "},{"type":"codeVoice","code":"actionGroup"},{"type":"text","text":" on "},{"type":"codeVoice","code":"connection"},{"type":"text","text":" at "},{"type":"codeVoice","code":"objectPath"},{"type":"text","text":"."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/DBusConnectionProtocol\/exportActionGroup(objectPath:actionGroup:)","kind":"symbol","type":"topic","url":"\/documentation\/gio\/dbusconnectionprotocol\/exportactiongroup(objectpath:actiongroup:)"},"doc://GIO/documentation/GIO/IOStream":{"role":"symbol","title":"IOStream","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"IOStream"}],"abstract":[{"type":"text","text":"GIOStream represents an object that has both read and write streams."},{"type":"text","text":" "},{"type":"text","text":"Generally the two streams act as separate input and output streams,"},{"type":"text","text":" "},{"type":"text","text":"but they share some common resources and state. For instance, for"},{"type":"text","text":" "},{"type":"text","text":"seekable streams, both streams may use the same position."}],"identifier":"doc:\/\/GIO\/documentation\/GIO\/IOStream","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"IOStream"}],"url":"\/documentation\/gio\/iostream"}}}