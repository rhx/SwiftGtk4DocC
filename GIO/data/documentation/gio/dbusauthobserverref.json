through "},{"type":"codeVoice","code":"DBusAuthObserverProtocol"},{"type":"text","text":" conformance."},{"type":"text","text":" "},{"type":"text","text":"Use "},{"type":"codeVoice","code":"DBusAuthObserverRef"},{"type":"text","text":" only as an "},{"type":"codeVoice","code":"unowned"},{"type":"text","text":" reference to an existing "},{"type":"codeVoice","code":"GDBusAuthObserver"},{"type":"text","text":" instance."}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/gio\/dbusauthobserverref"],"traits":[{"interfaceLanguage":"swift"}]}],"relationshipsSections":[{"identifiers":["doc:\/\/GIO\/documentation\/GIO\/DBusAuthObserverProtocol","doc:\/\/GIO\/4GLib14PointerWrapperP","doc:\/\/GIO\/10GLibObject14GWeakCapturingP","doc:\/\/GIO\/10GLibObject0B8ProtocolP"],"kind":"relationships","title":"Conforms To","type":"conformsTo"}],"identifier":{"url":"doc:\/\/GIO\/documentation\/GIO\/DBusAuthObserverRef","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"GDBusAuthObserver"},{"type":"text","text":" type provides a mechanism for participating"},{"type":"text","text":" "},{"type":"text","text":"in how a "},{"type":"codeVoice","code":"GDBusServer"},{"type":"text","text":" (or a "},{"type":"codeVoice","code":"GDBusConnection"},{"type":"text","text":") authenticates remote"},{"type":"text","text":" "},{"type":"text","text":"peers. Simply instantiate a "},{"type":"codeVoice","code":"GDBusAuthObserver"},{"type":"text","text":" and connect to the"},{"type":"text","text":" "},{"type":"text","text":"signals you are interested in. Note that new signals may be added"},{"type":"text","text":" "},{"type":"text","text":"in the future"}],"kind":"symbol","metadata":{"fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"DBusAuthObserverRef"}],"title":"DBusAuthObserverRef","roleHeading":"Structure","role":"symbol","symbolKind":"struct","ext