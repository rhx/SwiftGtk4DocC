,{"type":"codeVoice","code":"GAppLaunchContext::launched"},{"type":"text","text":" signal emission; after the signal is emitted,"},{"type":"text","text":" "},{"type":"text","text":"GLib will call "},{"type":"codeVoice","code":"g_spawn_close_pid()"},{"type":"text","text":". If you need to keep the "},{"type":"codeVoice","code":"GPid"},{"type":"text","text":" after the"},{"type":"text","text":" "},{"type":"text","text":"signal has been emitted, then you can duplicate "},{"type":"codeVoice","code":"pid"},{"type":"text","text":" using "},{"type":"codeVoice","code":"DuplicateHandle()"},{"type":"text","text":"."}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/gio\/applaunchcontextsignalname\/launched"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/GIO\/documentation\/GIO\/AppLaunchContextSignalName\/launched","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"GAppLaunchContext::launched"},{"type":"text","text":" signal is emitted when a "},{"type":"codeVoice","code":"GAppInfo"},{"type":"text","text":" is successfully"},{"type":"text","text":" "},{"type":"text","text":"launched."}],"kind":"symbol","metadata":{"fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"launched"}],"title":"AppLaunchContextSignalName.launched","roleHeading":"Case","role":"symbol","symbolKind":"case","externalID":"s:3GIO26AppLaunchContextSignalNameO8launchedyA2CmF","modules":[{"name":"GIO"}]},"hierarchy":{"paths":[["doc:\/\/GIO\/documentation\/GIO","doc:\/\/GIO\/documentation\/GIO\/AppLaunchContextSignalName"]]},"references":{"doc://GIO/documentation/GIO/AppLaunchContextSignalName/launched":{"role":"symbol","title":"AppLaunchContextSignalName.launched","fragments":[{"kind":"keyword","text":"case"},{"kind":"text","text":" "},{"kind":"identifier","text":"launched"}],"abstract":[{"type":"text","text":"The "},{"type":"codeV