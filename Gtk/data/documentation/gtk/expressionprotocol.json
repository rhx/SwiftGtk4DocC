"doc:\/\/Gtk\/documentation\/Gtk\/PropertyExpressionProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"PropertyExpressionProtocol"}],"url":"\/documentation\/gtk\/propertyexpressionprotocol"},"doc://Gtk/documentation/Gtk/ExpressionProtocol/valueSetExpression(value:)":{"role":"symbol","title":"valueSetExpression(value:)","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"valueSetExpression"},{"kind":"text","text":"<"},{"kind":"genericParameter","text":"GLibObjectValueT"},{"kind":"text","text":">("},{"kind":"externalParam","text":"value"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"GLibObjectValueT"},{"kind":"text","text":")"}],"abstract":[{"type":"text","text":"Stores the given "},{"type":"codeVoice","code":"GtkExpression"},{"type":"text","text":" inside "},{"type":"codeVoice","code":"value"},{"type":"text","text":"."}],"identifier":"doc:\/\/Gtk\/documentation\/Gtk\/ExpressionProtocol\/valueSetExpression(value:)","kind":"symbol","type":"topic","url":"\/documentation\/gtk\/expressionprotocol\/valuesetexpression(value:)"},"doc://Gtk/documentation/Gtk/ExpressionProtocol":{"role":"symbol","title":"ExpressionProtocol","fragments":[{"kind":"keyword","text":"protocol"},{"kind":"text","text":" "},{"kind":"identifier","text":"ExpressionProtocol"}],"abstract":[{"type":"codeVoice","code":"GtkExpression"},{"type":"text","text":" provides a way to describe references to values."}],"identifier":"doc:\/\/Gtk\/documentation\/Gtk\/ExpressionProtocol","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"ExpressionProtocol"}],"url":"\/documentation\/gtk\/expressionprotocol"},"doc://Gtk/documentation/Gtk/ClosureExpressionRef":{"role":"symbol","title":"ClosureExpressionRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"ClosureExpressionRef"}],"abstract":[{"type":"text","text":"An expression using a custo