nRef":{"role":"symbol","title":"PropertyExpressionRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"PropertyExpressionRef"}],"abstract":[{"type":"text","text":"A "},{"type":"codeVoice","code":"GObject"},{"type":"text","text":" property value in a "},{"type":"codeVoice","code":"GtkExpression"},{"type":"text","text":"."}],"identifier":"doc:\/\/Gtk\/documentation\/Gtk\/PropertyExpressionRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"PropertyExpressionRef"}],"url":"\/documentation\/gtk\/propertyexpressionref"},"doc://Gtk/documentation/Gtk/PropertyExpression":{"role":"symbol","title":"PropertyExpression","fragments":[{"kind":"keyword","text":"class"},{"kind":"text","text":" "},{"kind":"identifier","text":"PropertyExpression"}],"abstract":[{"type":"text","text":"A "},{"type":"codeVoice","code":"GObject"},{"type":"text","text":" property value in a "},{"type":"codeVoice","code":"GtkExpression"},{"type":"text","text":"."}],"identifier":"doc:\/\/Gtk\/documentation\/Gtk\/PropertyExpression","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"PropertyExpression"}],"url":"\/documentation\/gtk\/propertyexpression"},"doc://Gtk/documentation/Gtk/ExpressionProtocol/getValueType()":{"role":"symbol","title":"getValueType()","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"getValueType"},{"kind":"text","text":"() -> "},{"kind":"typeIdentifier","text":"GType","preciseIdentifier":"c:@T@GType"}],"abstract":[{"type":"text","text":"Gets the "},{"type":"codeVoice","code":"GType"},{"type":"text","text":" that this expression evaluates to."}],"identifier":"doc:\/\/Gtk\/documentation\/Gtk\/ExpressionProtocol\/getValueType()","kind":"symbol","type":"topic","url":"\/documentation\/gtk\/expressionprotocol\/getvaluetype()"},"doc://Gtk/documentation/Gtk/ExpressionProtocol/valueTakeExpression(value:)":{"role":"symbol","title":"v