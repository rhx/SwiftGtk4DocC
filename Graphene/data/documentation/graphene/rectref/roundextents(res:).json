s"},{"type":"text","text":" "},{"type":"text","text":"of the original rectangle."}],"identifier":"doc:\/\/Graphene\/documentation\/Graphene\/RectRef\/roundExtents(res:)","kind":"symbol","type":"topic","url":"\/documentation\/graphene\/rectref\/roundextents(res:)"},"doc://Graphene/documentation/Graphene/RectRef":{"role":"symbol","title":"RectRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"RectRef"}],"abstract":[{"type":"text","text":"The location and size of a rectangle region."}],"identifier":"doc:\/\/Graphene\/documentation\/Graphene\/RectRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"RectRef"}],"url":"\/documentation\/graphene\/rectref"}}}