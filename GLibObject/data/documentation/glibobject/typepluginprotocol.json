Plugin"},{"type":"text","text":" structure pointed to by "},{"type":"codeVoice","code":"plugin"},{"type":"text","text":" to manage the type and its"},{"type":"text","text":" "},{"type":"text","text":"instances (if not abstract).  The value of "},{"type":"codeVoice","code":"flags"},{"type":"text","text":" determines the nature"},{"type":"text","text":" "},{"type":"text","text":"(e.g. abstract or not) of the type."}],"identifier":"doc:\/\/GLibObject\/documentation\/GLibObject\/TypePluginProtocol\/typeRegisterDynamic(parentType:typeName:flags:)","kind":"symbol","type":"topic","url":"\/documentation\/glibobject\/typepluginprotocol\/typeregisterdynamic(parenttype:typename:flags:)"},"doc://GLibObject/documentation/GLibObject/TypePluginProtocol/type_plugin_ptr-1f7zf":{"defaultImplementations":1,"role":"symbol","title":"type_plugin_ptr","fragments":[{"kind":"keyword","text":"var"},{"kind":"text","text":" "},{"kind":"identifier","text":"type_plugin_ptr"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"UnsafeMutablePointer","preciseIdentifier":"s:Sp"},{"kind":"text","text":"<"},{"kind":"typeIdentifier","text":"GTypePlugin"},{"kind":"text","text":">!"}],"abstract":[{"type":"text","text":"Typed pointer to the underlying "},{"type":"codeVoice","code":"GTypePlugin"},{"type":"text","text":" instance."}],"identifier":"doc:\/\/GLibObject\/documentation\/GLibObject\/TypePluginProtocol\/type_plugin_ptr-1f7zf","kind":"symbol","required":true,"type":"topic","url":"\/documentation\/glibobject\/typepluginprotocol\/type_plugin_ptr-1f7zf"},"doc://GLibObject/documentation/GLibObject/TypePluginProtocol/use()":{"role":"symbol","title":"use()","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"use"},{"kind":"text","text":"()"}],"abstract":[{"type":"text","text":"Calls the "},{"type":"codeVoice","code":"use_plugin"},{"type":"text","text":" function from the "},{"type":"codeVoice","code":"GTypePluginClass"},{"type":"text","text":" of