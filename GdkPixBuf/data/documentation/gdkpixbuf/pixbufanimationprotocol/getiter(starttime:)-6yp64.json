 "},{"kind":"identifier","text":"PixbufLoader"}],"abstract":[{"type":"text","text":"Incremental image loader."}],"identifier":"doc:\/\/GdkPixBuf\/documentation\/GdkPixBuf\/PixbufLoader","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"PixbufLoader"}],"url":"\/documentation\/gdkpixbuf\/pixbufloader"},"doc://GdkPixBuf/documentation/GdkPixBuf/PixbufAnimationIterRef":{"role":"symbol","title":"PixbufAnimationIterRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"PixbufAnimationIterRef"}],"abstract":[{"type":"text","text":"An opaque object representing an iterator which points to a"},{"type":"text","text":" "},{"type":"text","text":"certain position in an animation."}],"identifier":"doc:\/\/GdkPixBuf\/documentation\/GdkPixBuf\/PixbufAnimationIterRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"PixbufAnimationIterRef"}],"url":"\/documentation\/gdkpixbuf\/pixbufanimationiterref"}}}