text":" "},{"type":"text","text":"touching-but-not-overlapping after rounding to device units, pass them in"},{"type":"text","text":" "},{"type":"text","text":"as "},{"type":"codeVoice","code":"nearest"},{"type":"text","text":"."}]}]}],"schemaVersion":{"major":0,"minor":3,"patch":0},"sections":[],"variants":[{"paths":["\/documentation\/pango\/rectangle\/extentstopixels(nearest:)"],"traits":[{"interfaceLanguage":"swift"}]}],"identifier":{"url":"doc:\/\/Pango\/documentation\/Pango\/Rectangle\/extentsToPixels(nearest:)","interfaceLanguage":"swift"},"abstract":[{"type":"text","text":"Converts extents from Pango units to device units."}],"kind":"symbol","metadata":{"role":"symbol","title":"extentsToPixels(nearest:)","roleHeading":"Instance Method","fragments":[{"kind":"keyword","text":"func"},{"kind":"text","text":" "},{"kind":"identifier","text":"extentsToPixels"},{"kind":"text","text":"("},{"kind":"externalParam","text":"nearest"},{"kind":"text","text":": "},{"kind":"typeIdentifier","text":"RectangleRef","preciseIdentifier":"s:5Pango12RectangleRefV"},{"kind":"text","text":"?)"}],"symbolKind":"method","externalID":"s:5Pango17RectangleProtocolPAAE15extentsToPixels7nearestyAA0B3RefVSg_tF::SYNTHESIZED::s:5Pango9RectangleC","extendedModule":"Pango","modules":[{"name":"Pango"}]},"hierarchy":{"paths":[["doc:\/\/Pango\/documentation\/Pango","doc:\/\/Pango\/documentation\/Pango\/Rectangle","doc:\/\/Pango\/documentation\/Pango\/Rectangle\/RectangleProtocol-Implementations"]]},"references":{"doc://Pango/documentation/Pango/RectangleRef":{"role":"symbol","title":"RectangleRef","fragments":[{"kind":"keyword","text":"struct"},{"kind":"text","text":" "},{"kind":"identifier","text":"RectangleRef"}],"abstract":[{"type":"text","text":"The "},{"type":"codeVoice","code":"PangoRectangle"},{"type":"text","text":" structure represents a rectangle."}],"identifier":"doc:\/\/Pango\/documentation\/Pango\/RectangleRef","kind":"symbol","type":"topic","navigatorTitle":[{"kind":"identifier","text":"